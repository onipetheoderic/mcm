<!-- Custom Content -->
            <div class="row clearfix">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="card">
                        <div class="header">
                            <h2>
                                All Messages from Visitors In Home Page
                            </h2>
                           
                        </div>
                        <div class="body">
                            <div class="row">
                                {{#each school_msgs}}
                                 <div class="col-sm-6 col-md-6">
                                    <div class="thumbnail">
                                        
                                        <div class="caption">
                                            <center>
                                            <h3>{{this.email}}</h3>
                                             <h3>{{this.name}}</h3>
                                        </center>
                                            <p>
                                            {{this.message}}
                                            </p>
                                           
                                     
                                           
                                        </div>
                                    </div>
                                </div>
                                {{/each}}     
                            </div>
                        </div>
                    </div>
                </div>
            </div>

          